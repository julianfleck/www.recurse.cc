{
  "name": "recurse.cc-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:www": "pnpm --filter @recurse/www dev",
    "dev:docs": "pnpm --filter docs.recurse.cc dev",
    "dev:dashboard": "pnpm --filter @recurse/dashboard dev",
    "dev:all": "pnpm --parallel --filter '@recurse/*' dev",
    "build": "pnpm -r build",
    "build:check": "node scripts/check-builds.mjs",
    "lint": "pnpm -r exec ultracite check",
    "fix": "pnpm -r exec ultracite fix"
  },
  "devDependencies": {
    "shadcn": "^3.5.0",
    "typescript": "^5.9.2"
  }
}
