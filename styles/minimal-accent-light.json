  "tokenColors": [
    {
      "scope": [
        "entity.name.function",
        "entity.name.function.member",
        "support.function",
        "support.class.builtin",
        "support.class.console",
        "support.function.builtin",
        "support.function.math",
        "support.function.array",
        "support.function.object",
        "support.function.json",
        "support.function.promise",
        "support.function.dom",
        "support.function.console",
        "support.function",
        "meta.method-call entity.name.function",
        "meta.method.call entity.name.function"
      ],
      "settings": {
        "foreground": "#4B0082",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type.function.arrow", "keyword.operator.arrow"],
      "settings": {
        "foreground": "#797c0c"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.name.type.interface",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#4B0082",
        "background": "#b98eff15"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.readwrite",
        "variable.declaration",
        "meta.definition.variable variable.other",
        "meta.import variable.other",
        "variable.enum.member",
        "variable.other.enum.member",
        "variable.other.boolean"
      ],
      "settings": {
        "foreground": "#4B0082"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.name.type.interface",
        "support.class",
        "support.type",
        "meta.import variable.other",
        "variable.other.readwrite.alias",
        "meta.import.default variable.other",
        "meta.definition.variable entity.name.type",
        "meta.import entity.name.type"
      ],
      "settings": {
        "foreground": "#797c0c"
      }
    },
    {
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.return",
        "keyword.control.throw",
        "keyword.control.try",
        "keyword.control.break",
        "keyword.control.continue"
      ],
      "settings": {
        "foreground": "#B7BC12",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#797c0c"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "meta.tag.attributes"],
      "settings": {
        "foreground": "#4B0082"
      }
    },
    {
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin"
      ],
      "settings": {
        "foreground": "#797c0c"
      }
    },
    {
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#b98eff"
      }
    },
    {
      "scope": ["variable", "parameter"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": ["meta.embedded", "punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#4B0082"
      }
    },
    {
      "scope": [
        "comment",
        "comment.block.documentation",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#b0a5c3",
        "fontStyle": "italic"
      }
    }
  ],
