        "typescriptreact": "#797c0c",

        "config": "#4B0082",
        "json": "#4B0082",
        "package": "#4B0082",
        "git": "#4B0082"
      }
    }
  },
  "explorer.decorations.colors": {
    "folder-icon": "#666666",
    "folder-icon-expanded": "#808080"
  }
}
