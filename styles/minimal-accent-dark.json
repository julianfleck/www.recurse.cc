  "tokenColors": [
    {
      "scope": [
        "entity.name.function.member",
        "support.function",
        "support.class.builtin",
        "support.class.console",
        "support.function.builtin",
        "support.function.math",
        "support.function.array",
        "support.function.object",
        "support.function.json",
        "support.function.promise",
        "support.function.dom",
        "support.function.console"
      ],
      "settings": {
        "foreground": "#8D87CF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "meta.method-call entity.name.function",
        "meta.method.call entity.name.function"
      ],
      "settings": {
        "foreground": "#8D87CF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage.type.function.arrow", "keyword.operator.arrow"],
      "settings": {
        "foreground": "#BBC726"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "meta.parameters"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.name.type.interface",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#BBC726",
        "background": "#BBC72615"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.readwrite",
        "variable.declaration",
        "meta.definition.variable variable.other"
      ],
      "settings": {
        "foreground": "#A6C82E"
      }
    },
    {
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.switch",
        "keyword.control.case",
        "keyword.control.return",
        "keyword.control.try",
        "keyword.control.catch",
        "keyword.control.finally"
      ],
      "settings": {
        "foreground": "#E57637",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#BBC726"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "meta.tag.attributes"],
      "settings": {
        "foreground": "#8D87CF"
      }
    },
    {
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "scope": [
        "comment",
        "comment.block.documentation",
        "comment.line.double-slash",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4A3D66",
        "fontStyle": "italic"
      }
    }
  ],
  "gitDecoration.submoduleForeground": "#666666",
  "gitDecoration.modifiedResourceForeground": "#BBC726",
  "gitDecoration.untrackedResourceForeground": "#A6C82E",
  "gitDecoration.conflictingResourceForeground": "#ff6666",
  "gitDecoration.ignoredResourceForeground": "#4A3D66",
  "gitDecoration.deletedResourceForeground": "#ff6666",
  "gitDecoration.stageModifiedResourceForeground": "#BBC726",
  "gitDecoration.dirtyForeground": "#A6C82E",
